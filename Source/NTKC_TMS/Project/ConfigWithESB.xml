<Config>
	<!-- Doan nay cau hinh de ket noi voi webservices cua ESB -->
	<WsUrl>http://localhost:1387/Service1.asmx</WsUrl>
	<SoapAction>http://tempuri.org/CheckInput</SoapAction>
	<XmlRequest>		
		<?xml version="1.0" encoding="utf-8"?>
		<soap:Envelope xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/">
			<soap:Body>
				<CheckInput xmlns="http://tempuri.org/">
					<sInPut>string</sInPut>
				</CheckInput>
			</soap:Body>
		</soap:Envelope>
	</XmlRequest>	
	<TaxOffcice>00101</TaxOffcice>
	<ParamName>sInPut</ParamName>
	
	<!-- Doan nay cau hinh cho phan Header cua message gui tin len ESB -->
	<VERSION>1.0</VERSION>
	<SENDER_CODE>NTK</SENDER_CODE>
	<SENDER_NAME>Hệ thống nhận tờ khai mã vạch</SENDER_NAME>
	<RECEIVER_CODE>TMS</RECEIVER_CODE>
	<RECEIVER_NAME>Hệ thống Quản lý thuế tập trung</RECEIVER_NAME>
	<TRAN_CODE>00006</TRAN_CODE>
	<ORIGINAL_CODE>NTK</ORIGINAL_CODE>
	<ORIGINAL_NAME>Hệ thống nhận tờ khai mã vạch</ORIGINAL_NAME>
	<CODE_OFFICE>HAN</CODE_OFFICE> <!-- ma Cuc hoac ma Chi Cuc -->
	
	
	<!-- Doan nay cau hinh cho phan Header cua to khai -->
	<maCQTNoiNop>HAN</maCQTNoiNop>
	<tenCQTNoiNop>Hà Nội</tenCQTNoiNop>
	
	<!-- Doan nay cau hinh cho phan MQ push data len ESB -->
	<queue_manager_name>ESB.INT.QMGR</queue_manager_name>
	<queue_name>INT.INBOX.QUEUE</queue_name>
	
	<!-- Cac cau hinh khac -->
	
</Config>