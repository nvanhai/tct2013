	<xs:redefine schemaLocation="../TKhaiThue.xsd">
		<xs:complexType name="CTieuTKhaiChinhType">
			<xs:complexContent>
				<xs:extension base="CTieuTKhaiChinhType">
					<xs:sequence>
						<xs:element name="tieuMucHachToan" type="tieuMucHT_empty">
							<xs:annotation>
								<xs:documentation>Tiểu mục hạch toán</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="dnQuiMoVuaVaNho" type="xs:boolean" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh nghiệp có quy mô nhỏ và vừa</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="dnCoCSoHToanPThuoc" type="xs:boolean" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh nghiệp có cơ sở sản xuất hạch toán phụ thuộc</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct21" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh thu phát sinh trong kỳ </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct22" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh thu theo thuế suất không ưu đãi: </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct22a" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh thu theo thuế suất không ưu đãi: 20% </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct22b" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh thu theo thuế suất không ưu đãi: 22% </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct22c" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh thu theo thuế suất không ưu đãi khác</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct23" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Doanh thu của hoạt động được ưu đãi về thuế suất</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct24" type="xs:decimal" nillable="true">
							<xs:annotation>
								<xs:documentation>Tỷ lệ thu nhập chịu thuế trên doanh thu (%)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct25a" type="xs:decimal" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế suất không ưu đãi: 20% </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct25b" type="xs:decimal" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế suất không ưu đãi: 22% </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct25c" type="xs:decimal" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế suất không ưu đãi khác: </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct26" type="xs:decimal" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế suất ưu đãi (%)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct27" type="xs:long" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế thu nhập doanh nghiệp (TNDN) phát sinh trong kỳ ([27]= [28]+[29])</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct28" type="xs:long" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế TNDN của hoạt động không ưu đãi  ([28]=[22a]x[24] x [25a]+[22b]x[24] x [25b]+[22c]x[24] x [25c])</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct29" type="xs:long" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế TNDN của hoạt động được ưu đãi về thuế suất ([29]= [23]x[24]x[26])</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct30" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế thu nhập doanh nghiệp dự kiến miễn, giảm</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct30a" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Trong đó: + số thuế TNDN được miễn, giảm theo Hiệp định</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct30b" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>                + Miễn, giảm khác ngoài Luật Thuế TNDN (nếu có)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="duocHuongMucThueUuDai" type="xs:string" nillable="true">
							<xs:annotation>
								<xs:documentation>Được hưởng nhiều mức ưu đãi,Nếu check thì ghi “x” ngược lại thì để trống</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct31" type="xs:long" nillable="true">
							<xs:annotation>
								<xs:documentation>Thuế TNDN phải nộp trong kỳ ([31]=[27]–[30])</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct32" type="xs:boolean" nillable="true">
							<xs:annotation>
								<xs:documentation>Đối tượng được gia hạn    </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct32a" type="xs:string" nillable="true">
							<xs:annotation>
								<xs:documentation>Mã lý do gia hạn</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct32b" type="xs:date" nillable="true">
							<xs:annotation>
								<xs:documentation>Thời hạn được gia hạn</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct32c" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Số thuế TNDN được gia hạn </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ct32d" type="xs:nonNegativeInteger" nillable="true">
							<xs:annotation>
								<xs:documentation>Số thuế TNDN không được gia hạn</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
