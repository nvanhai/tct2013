						</xs:element>
						<xs:element name="TongThueTNCNDaKhauTru">
							<xs:annotation>
								<xs:documentation>5. Tổng số thuế TNCN đã khấu trừ </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ct34" type="xs:nonNegativeInteger" nillable="true">
										<xs:annotation>
											<xs:documentation>Tổng số thuế TNCN đã khấu trừ [34]=[35]+[36]+[37]</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ct35" type="xs:nonNegativeInteger" nillable="true">
										<xs:annotation>
											<xs:documentation>Cá nhân cư trú có hợp đồng lao động</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ct36" type="xs:nonNegativeInteger" nillable="true">
										<xs:annotation>
											<xs:documentation>Cá nhân cư trú không có hợp đồng lao động</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ct37" type="xs:nonNegativeInteger" nillable="true">
										<xs:annotation>
											<xs:documentation>Cá nhân không cư trú</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
