						<xs:element name="TongSoNguoiLaoDong">
							<xs:annotation>
								<xs:documentation>1. Tổng số người lao động:</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ct21" type="xs:integer" nillable="true">
										<xs:annotation>
											<xs:documentation>Tổng số người lao động</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ct22" type="xs:integer" nillable="true">
										<xs:annotation>
											<xs:documentation>Trong đó: Cá nhân cư trú có hợp đồng lao động</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="TongSoCaNhanDaKhauTru">
							<xs:annotation>
								<xs:documentation>2. Tổng số cá nhân đã khấu trừ thuế </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ct23" type="xs:nonNegativeInteger" nillable="true">
										<xs:annotation>
											<xs:documentation>Tổng số cá nhân đã khấu trừ thuế [23]=[24]+[25]</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ct24" type="xs:nonNegativeInteger" nillable="true">
										<xs:annotation>
											<xs:documentation>Cá nhân cư trú</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ct25" type="xs:nonNegativeInteger" nillable="true">
										<xs:annotation>
											<xs:documentation>Cá nhân không cư trú</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="TongTNCTTraChoCaNhan">
							<xs:annotation>
								<xs:documentation>3. Tổng thu nhập chịu thuế (TNCT) trả cho cá nhân </xs:documentation>
							</xs:annotation>
							<xs:complexType>
